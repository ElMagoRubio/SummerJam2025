{
  "song_bpm": "120",
  "tolerance_time": "1 / (file_bpm / 60.0)", <- variable externa
  "tolerance_modifier": "1, 2, 3...", <- Variable externa
  "beats": [
    {
      "original_time": "tiempo_nota_1",
      "start_time": "tiempo_nota_1 - (1/2^tolerance_modifier * tolerance_time)", <- Cuenta externa
      "end_time": "tiempo_nota_1 + (1/2^tolerance_modifier * tolerance_time)", <- Cuenta externa
      "type": "KEY_N"
    },
    {
      "original_time": "tiempo_nota_2",
      "start_time": "tiempo_nota_2 - (2^tolerance_modifier * tolerance_time)",
      "end_time": "tiempo_nota_2 + (2^tolerance_modifier * tolerance_time)",
      "type": "KEY_N"
    }
  ]
}